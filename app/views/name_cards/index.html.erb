<h1>Listing name_cards</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Adress</th>
      <th>Tel</th>
      <th>Fax</th>
      <th>Url</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @name_cards.each do |name_card| %>
      <tr>
        <td><%= name_card.name %></td>
        <td><%= name_card.address %></td>
        <td><%= name_card.tel %></td>
        <td><%= name_card.fax %></td>
        <td><%= name_card.url %></td>
        <td>
          <%= link_to 'Show', company_group_user_name_card_path(name_card.user.group.company_id, name_card.user.group_id, name_card.user.id, name_card.id) %><br />
          <%= link_to 'Edit', edit_company_group_user_name_card_path(name_card.user.group.company_id, name_card.user.group_id, name_card.user.id, name_card.id) %><br />
          <%= link_to 'Destroy', company_group_user_name_card_path(name_card.user.group.company_id, name_card.user.group_id, name_card.user.id, name_card.id), method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= paginate @name_cards %>
<%= link_to 'New name_card', new_company_group_user_name_card_path %>
<%= link_to 'Back', company_group_users_path %>
